{
  "log_level": "INFO",

  "mqtt_address": "localhost",
  "mqtt_port": 1883,
  "mqtt_user": null,
  "mqtt_password": null,
  "mqtt_topic": "#",
  "skip_empty_fields": 1,

  "influxdb_enabled": 1,
  "influxdb_url": "http://localhost:8086",
  "influxdb_token": "replace-with-influx-token",
  "influxdb_org": "replace-with-influx-org",
  "influxdb_bucket": "replace-with-influx-bucket",
  "influx_measurement": "mqtt_data",

  "signalk_enabled": 0,
  "signalk_server_url": "ws://localhost:3000/signalk/v1/stream",
  "signalk_token": "",
  "signalk_context_prefix": "meteo",
  "signalk_source_label": "sensor-network-collector",
  "signalk_path_map": {
    "TempOut": "environment.outside.temperature",
    "HumOut": "environment.outside.humidity",
    "Barometer": "environment.outside.pressure",
    "WindSpeed": "environment.wind.speedApparent",
    "WindDir": "environment.wind.angleApparent"
  },

  "storage_root": null
}
